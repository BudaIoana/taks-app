{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "allowJs": true,
      "jsx": "react-jsx",
      "outDir": "./dist",
      "moduleResolution": "node",
      "isolatedModules": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "noEmit": true
    },
    "include": [
      "./src/**/*"
    ]
  }